<?xml version='1.0' encoding='utf-8'?>
<odoo noupdate="1">
    <!-- model.name cron -->
    <record id="model_name_ir_cron" forcecreate="True" model="ir.cron">
        <field name="name">Ajoute 1 aux Seats Taken</field>
        <field name="model_id" ref="carpooling.model_carpooling_carpooling"/>
        <field name="state">code</field>
        <field name="code">model._run_cron()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(hours=2)).strftime('%Y-%m-%d %H:%M:%S')"/>

    </record>
</odoo>